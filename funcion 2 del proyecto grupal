// Funcion para convertit centimetros a pulgadas
#include <stdio.h>
int cm_a_pulgadas(int centimetros){
    return centimetros /2.54;
}
//funcion para convertir pulgadas a centimetros
int pulgadas_a_cm(int pulgadass){
    return pulgadas *2.54;
    return 0;
}
